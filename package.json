{
  "name": "apt-logger",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@date-io/date-fns": "^1.3.9",
    "@material-ui/core": "^4.3.2",
    "@material-ui/icons": "^4.2.1",
    "@types/jest": "24.0.17",
    "@types/node": "12.7.2",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.8.5",
    "date-fns": "^2.0.0-beta.5",
    "dexie": "^2.0.4",
    "electron-is-dev": "^1.1.0",
    "material-table": "^1.50.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1",
    "typeface-roboto": "^0.0.75",
    "typescript": "^3.5.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "electron-pack": "build -mw"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.11",
    "@rescripts/rescript-env": "^0.0.10",
    "concurrently": "^4.1.2",
    "electron": "^7.2.4",
    "electron-builder": "^21.2.0",
    "wait-on": "^3.3.0"
  },
  "author": {
    "name": "Benjamin Goodman",
    "email": "b@thebgoodman.com",
    "url": "https://thebgoodman.com"
  },
  "build": {
    "appId": "com.thebgoodman.apt-logger",
    "productName": "Apt Logs",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.Productivity.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
